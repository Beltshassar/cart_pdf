{
    "name": "extcode/cart-pdf",
    "type": "typo3-cms-extension",
    "description": "Shopping Cart(s) for TYPO3 - PDF Renderer for Invoices and Delivery Notes",
    "homepage": "http://wt-cart.com",
    "license": [
        "GPL-2.0+"
    ],
    "keywords": [
        "TYPO3 CMS",
        "Shopping Cart",
        "PDF",
        "cart"
    ],
    "support": {
        "issues": "https://github.com/extcode/cart_pdf/issues"
    },
    "require": {
        "php": ">=5.6.0",
        "typo3/cms": "^6.2 || ^7.6 || ^8.7",
        "extcode/cart": ">=3.2.1",
        "extcode/tcpdf": ">=0.4.0"
    },
    "require-dev": {
        "nimut/testing-framework": "^1.0",
        "mikey179/vfsStream": "~1.6.0",
        "phpunit/phpunit": "~4.8.0",
        "friendsofphp/php-cs-fixer": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Extcode\\CartPdf\\": "Classes"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Extcode\\CartPdf\\Tests\\": "Tests",
            "TYPO3\\CMS\\Core\\Tests\\": ".Build/vendor/typo3/cms/typo3/sysext/core/Tests/",
            "TYPO3\\CMS\\Fluid\\Tests\\": ".Build/vendor/typo3/cms/typo3/sysext/fluid/Tests/"
        }
    },
    "replace": {
        "cart_pdf": "self.version"
    },
    "config": {
        "vendor-dir": ".Build/vendor",
        "bin-dir": ".Build/bin"
    },
    "extra": {
        "typo3/cms": {
            "cms-package-dir": "{$vendor-dir}/typo3/cms",
            "web-dir": ".Build/Web",
            "Package": {
                "partOfMinimalUsableSystem": true
            }
        }
    }
}